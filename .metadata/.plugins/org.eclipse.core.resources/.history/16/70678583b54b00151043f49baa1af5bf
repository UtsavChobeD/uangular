angular.module('myApp', []).controller(
		'namesCtrl',
		function($scope) {
			$scope.names = [ {
				name : 'Jani',
				country : 'Norway'
			}, {
				name : 'Hege',
				country : 'Sweden'
			}, {
				name : 'Kai',
				country : 'Denmark'
			} ];

			$http.get("http://www.w3schools.com/angular/customers.php")
					.success(function(response) {
						$scope.names = response.records;
					});
		});